{
    "customSyntax": "@stylelint/postcss-css-in-js",
    "rules": {
        "indentation": 4,
        "no-empty-source": null,
        "no-missing-end-of-source-newline": null,
        "value-keyword-case": null,
        "selector-type-no-unknown": [
            true,
            {
                "ignoreTypes": ["/-styled-mixin/"]
            }
        ]
    }
}
